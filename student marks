#include <stdio.h>
int main() {
    int m1, m2, m3, m4, m5;
    int total;
    float avg;
    printf("Enter 5 subject marks:\n");
    scanf("%d%d%d%d%d", &m1, &m2, &m3, &m4, &m5);
    total = m1 + m2 + m3 + m4 + m5;
    avg = total / 5.0;
    printf("Total = %d\n", total);
    printf("Average = %.2f\n", avg);
    if(avg >= 90)
        printf("Grade: A");
    else if(avg >= 75)
        printf("Grade: B");
    else if(avg >= 60)
        printf("Grade: C");
    else
        printf("Grade: Fail");
    return 0;
}
